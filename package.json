{
  "name": "priscilla-life",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "start": "pnpm --filter web start",
    "studio": "pnpm --filter studio dev",
    "type-check": "pnpm -r type-check",
    "populate": "pnpm --filter populate-sanity populate",
    "populate-with-images": "pnpm --filter populate-sanity populate-with-images",
    "cleanup": "pnpm --filter populate-sanity cleanup",
    "upload-images": "pnpm --filter populate-sanity upload-images",
    "fix-keys": "pnpm --filter populate-sanity fix-keys"
  }
}


